          <argument>
            <kind>1</kind>
            <string>///Movement

var movSpeed = 1.25;

global.playX = x;
global.playY = y;

var vertMovX, vertMovY;
var horzMovX, horzMovY;

vertMovX = lengthdir_x(vert*movSpeed,dirLook);
vertMovY = lengthdir_y(vert*movSpeed,dirLook);

horzMovX = lengthdir_x(horz*movSpeed,dirLook-90);
horzMovY = lengthdir_y(horz*movSpeed,dirLook-90);

var xP, yP;

xP = x; yP = y;

var canMoveIce = false;

/* Head Mode */

/*if headMode &lt; 1
{
headMode = 0;s
}

if keyboard_check_pressed(ord("H"))
{
headMode++;
}

switch (headMode)
{
    case (0):
    direction = direction;
        break;
    case (1):
    direction = round(direction/4)*4;
        break;
}*/

/* Looking Around */

if (instance_position(x,y,floorIce))
{
if (!isOnIce)
{
isOnIce = true;
    if (!audio_is_playing(slide))
    {
    audio_play_sound(slide,0,0);
    }
}
} else { isOnIce = false; }

if (!flashItemGetMsg)
{
dirVelo -= (keyboard_check(vk_left) - keyboard_check(vk_right)) * 2;
}

if (!keyboard_check(vk_left) &amp;&amp; !keyboard_check(vk_right))
{
dirVelo = lerp(dirVelo,0,0.5);
}
dirVelo = clamp(dirVelo, -5, 5);
direction += dirVelo;

/* Movement */
if (!flashItemGetMsg &amp;&amp; (!isOnIce || (currentItem == 3 &amp;&amp; global.hasSkates)))
{
dirLook = direction;
vert = keyboard_check(ord("W"))-keyboard_check(ord("S"));
horz = keyboard_check(ord("A"))-keyboard_check(ord("D"));
}
if (flashItemGetMsg)
{
dirLook = direction;
vert = 0;
horz = 0;
}

if (currentItem != 3 || !global.hasSkates) /* toDo, make skates */
{
if (isOnIce) /* Spend 1.5 hours on ice physics. */
{

if( (!(instance_place(x+vertMovX+horzMovX,y, wallCollide))) &amp;&amp; !((instance_place(x,y+vertMovY+horzMovY, wallCollide))) )
{
//do nothing?
hasSlid = true;
} else
{
vert = 0;
horz = 0;
hasSlid = false;
}

if !(vert != 0 || horz != 0)
{
vert = keyboard_check(ord("W"))-keyboard_check(ord("S"));
horz = keyboard_check(ord("A"))-keyboard_check(ord("D"));
dirLook = direction;

if (vert != 0 || horz != 0)
{
    if (!audio_is_playing(slide))
    {
    audio_play_sound(slide,0,0);
    }
}

} else { isOnIce = false; }

dirLook = round(dirLook/90)*90;

if (vert != 0)
{
horz = 0;
} else { vert = 0; }

}

}

if (vert != 0 || horz != 0)
{
bobCount += 0.375;
bobMult = lerp(bobMult,1,0.5);
} else { bobMult = lerp(bobMult,0,0.1); if (bobMult &lt;=0.05) {bobCount = 0;} }

if((!instance_place(x+vertMovX+horzMovX,y, wallCollide) &amp;&amp; (!instance_place(x+vertMovX+horzMovX,y, wallInvis)) &amp;&amp; !canClip) || keyboard_check(ord("Q")))
{
x+=vertMovX+horzMovX;
canMoveIce = false;
} else { x = xprevious; };

if((!instance_place(x,y+vertMovY+horzMovY, wallCollide) &amp;&amp; (!instance_place(x,y+vertMovY+horzMovY, wallInvis)) &amp;&amp; !canClip) || keyboard_check(ord("Q")))
{
y+=vertMovY+horzMovY;
canMoveIce = false;
} else { y = yprevious; };
