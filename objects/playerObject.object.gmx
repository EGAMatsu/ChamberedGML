            <kind>1</kind>
            <string>///Interface
if (!surface_exists(surf_UI)) {
surf_UI = surface_create(160,120);
}

currentItem = clamp(currentItem,0,7);

switch (keyboard_key)
{
    case (ord("1")):
        currentItem=0;
        break;
    case (ord("2")):
        currentItem=1;
        break;
